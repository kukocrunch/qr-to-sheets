{
    "name": "kukocrunch/qr-to-sheets",
    "description": "Laravel package that uses QR code to scan info and send its data to Google Sheets",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Ang",
            "email": "benjo.ang@gmail.com"
        }
    ],
    "require": {
        "simplesoftwareio/simple-qrcode": "^3.0"
    },
    "autoload": {
        "psr-4": {
